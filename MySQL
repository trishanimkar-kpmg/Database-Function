#Connector
import mysql.connector

mydb= mysql.connector.connect(user='scott', password='password',
                              host='127.0.0.1',
                              database='employees')
mydb.close()




#How to read the Data(Tabular)
my_database = mydb.cursor()
sql_statement = "SELECT * FROM CODESPEEDY"
my_database.execute(sql_statement)
output = my_database.fetchall()
for x in output:
  print(x)
